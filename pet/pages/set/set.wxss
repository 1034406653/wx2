/* pages/set/set.wxss */
.container{
  background: #f0f1f5;
}
.set{
  width: 100%;
  height: auto;
}
.set-li{
  width: 100%;
  height: 90rpx;
  line-height: 88rpx;
  border-bottom: 2rpx solid #f2f2f4; 
  padding: 0 30rpx;
  overflow: hidden;
   color: #333;
  font-size: 28rpx;
  background: #fff;
}
.li-title{
  width: auto;
  float: left;
}
.li-text{
  width: auto;
  float: right;
  padding-right: 30rpx;
}
.navBtn{
  width: 16rpx;
  height: 30rpx;
  float: right;
  margin-top: 30rpx;
}
.navBtn image{
  width: 16rpx;
  height: 30rpx;
}